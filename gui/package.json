{
  "name": "picorama-gui",
  "version": "1.2.0",
  "description": "GUI Front-End for CV Instances",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "deploy": "npm run build && rsync -r --delete --verbose --exclude 'photos' dist/. picorama:/home/public && scp deploy/.* picorama:/home/public",
    "lint": "eslint --ext .js,.vue src"
  },
  "author": "Ivo Janssen",
  "license": "UNLICENSED",
  "devDependencies": {
    "@appliedminds/eslint-config": "^1.3.0",
    "eslint": "^8.25.0",
    "eslint-plugin-vue": "^9.6.0",
    "sass": "^1.55.0",
    "sirv": "^2.0.2"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^3.1.2",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.29.3",
    "vite": "^3.1.8",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "@appliedminds",
      "plugin:vue/vue3-strongly-recommended"
    ],
    "rules": {
      "vue/html-indent": [
        "error",
        4
      ],
      "vue/multi-word-component-names": "off"
    }
  }
}
